$banner = $('#banner<%= @banner.id %>');
$updateForm = $('#update-form<%= @banner.id %>')
$indicator = $('#indicator<%= @banner.id %>')
console.log($banner);
console.log($updateForm);
$banner.slideUp(800);
$updateForm.slideUp(800);
